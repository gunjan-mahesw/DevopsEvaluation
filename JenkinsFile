pipeline{
    agent any{}
    tools{
        maven 'maven'
    }
    stages{
        stage('Build'){
            steps{
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('test'){
            steps{
                sh 'mvn test'
            }
        }
        stage('Deliver'){
            steps{
                sh './scripts/deliver.sh'
            }
        }
    }
}